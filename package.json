{
  "name": "yaktor",
  "version": "1.0.0-pre.0",
  "main": "index.js",
  "dependencies": {
    "async": "1.5.2",
    "commander": "2.0.0",
    "which": "1.0.5"
  },
  "devDependencies": {
    "backo": "1.0.1",
    "bluebird": "3.3.4",
    "yaktor-lang": "1.0.0-pre.0",
    "grunt": "1.0.1",
    "grunt-bump": "0.5.0",
    "grunt-cli": "0.1.13",
    "grunt-shell": "1.3.0",
    "js-yaml": "3.2.3",
    "load-grunt-tasks": "0.6.0",
    "mocha": "2.0.1",
    "mockgoose": "4.0.1",
    "mongoose": "4.4.12",
    "mongoose-pagination": "1.0.0",
    "mongoose-shortid": "0.5.0",
    "nice-xml": "0.1.1-rc2",
    "node-uuid": "1.4.2",
    "proxyquire": "1.0.1",
    "standard": "6.0.8",
    "standard-format": "2.1.1",
    "underscore": "1.7.0",
    "winston": "0.7.1"
  },
  "engines": {
    "node": ">=4.0.x"
  },
  "scripts": {
    "patch": "grunt release-patch",
    "minor": "grunt release-minor",
    "gen-src": "find . -name '*.cl' -o -name '*.dm' | xargs yaktor-lang",
    "test": "mocha -R spec test/*.test.js && npm run lint",
