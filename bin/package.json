  "scripts": {
    "start": "sudo -E node app.js",
    "format": "standard-format -w $(git diff --name-only HEAD | grep -e '\\.js$')",
    "lint": "standard $(git diff --name-only HEAD | grep -e '\\.js$')",
    "lint-staged": "standard $(git diff --name-only --cached | grep -e '\\.js$')",
    "migrate": "yaktor migrate",
    "gen-views": "grunt gen-views",
    "gen-docs": "(cd doc/; ruby buildAll.rb;)",
    "gen-src": "find . node_modules/yaktor -maxdepth 1 -name '*.cl' -o -name '*.dm' -follow | xargs yaktor-lang",
    "test": "LOG_LEVEL=warn istanbul cover _mocha -- --colors -R spec",
    "report": "find conversations/js  -name '*.js' | xargs plato -x '.gen.js|.def.js|\/types\/' -d plato",
    "envars": "cev --noprefix > config/custom-environment-variables.json"
  }
